{
    "compilerOptions": {
      "target": "esNext",
      "useDefineForClassFields": true,
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": false,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "noImplicitAny": true,
      "jsx": "react-jsx"
    },
    "include": ["src", "images.d.ts"],
    "exclude": [
      "node_modules"
    ]
  }